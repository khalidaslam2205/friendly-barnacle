<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" prefHeight="354.0" prefWidth="401.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testing48123.ProfileCreatorCSVFXMLController">
    <Label text="Runner ID:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <TextField fx:id="idField" GridPane.columnIndex="3" />

    <Label text="First Name:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <TextField fx:id="firstNameField" GridPane.columnIndex="3" GridPane.rowIndex="1" />

    <Label text="Last Name:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <TextField fx:id="lastNameField" GridPane.columnIndex="3" GridPane.rowIndex="2" />

    <Label text="Age:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <TextField fx:id="ageField" GridPane.columnIndex="3" GridPane.rowIndex="3" />

    <Label text="Gender:" GridPane.columnIndex="0" GridPane.rowIndex="5" />
    <ComboBox fx:id="genderComboBox" GridPane.columnIndex="3" GridPane.rowIndex="5">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Male" />
                <String fx:value="Female" />
                <String fx:value="Other" />
            </FXCollections>
        </items>
        <value>
            <String fx:value="Select a gender" />
        </value>
    </ComboBox>

    <Label text="Height:" GridPane.rowIndex="4" />
    <TextField fx:id="heightField" GridPane.columnIndex="3" GridPane.rowIndex="4" />

    <Label text="Weight:" GridPane.rowIndex="6" />
    <TextField fx:id="weightField" GridPane.columnIndex="3" GridPane.rowIndex="6" />

    <Label text="Password:" GridPane.rowIndex="7" />
    <PasswordField fx:id="passwordField" GridPane.columnIndex="3" GridPane.rowIndex="7" />

    <Button onAction="#handleSubmitButtonAction" text="Submit" GridPane.columnIndex="3" GridPane.rowIndex="8" />
    <Button id="backBtn" mnemonicParsing="false" onAction="#handleBackButtonAction" text="Back" GridPane.columnIndex="2" GridPane.rowIndex="9" />
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
</GridPane>
